{
  "env": {
    "production": {
      "plugins": [
        [
          "transform-react-remove-prop-types",
          {
            "mode": "remove",
            "removeImport": true
          }
        ],
        "transform-react-constant-elements"
      ]
    },
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    }
  },
  "compact": false,
  "plugins": [
    "syntax-dynamic-import",
    "transform-class-properties"
  ],
  "presets": [
    "stage-0",
    "stage-2",
    "react",
    [
      "env",
      {
        "modules": false,
        "loose": true,
        "useBuiltIns": true
      }
    ]
  ]
}
